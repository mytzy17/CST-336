<%- include('templates/header'); -%>
<div class='container'>
    <div class='jumbotron'>
        <h1> Search Results</h1>
        <button class='btn btn-info' onclick='location.href="/"'>Search Again</button>
        <ul>
            <% data.Search.forEach(function(movie){ %>
            <li>
                <img src=<%=movie.Poster%> />
                <div>
                    <h5></h5><%= movie.Title%></h5>
                    <p><%=movie.Year%></p>
                </div>
            </li>
            <% }) %>
        </ul>
    </div>
</div>
<%- include('templates/footer'); -%>